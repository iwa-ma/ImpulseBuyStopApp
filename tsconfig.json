{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx", // JSXサポートを追加
    "paths": {
      "@/*": ["./*"],
      "@assets/*": ["../assets/*"]
    },
    "baseUrl": "src",
  },
  "include": ["src"],
  "exclude": [
    "node_modules" // node_modulesを除外
  ]
}
